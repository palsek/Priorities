@model AspNetIdentityTry1.Models.User

@{
    ViewBag.Title = "DeleteUser";
}

<h2>Delete User</h2>

@using (Html.BeginForm())
{ 
    @Html.HiddenFor(m => m.UserName)    
    
    <p>
        <button type="submit" class="btn btn-default">Are you sure to delete @User.Identity.Name user?</button>
    </p>
}