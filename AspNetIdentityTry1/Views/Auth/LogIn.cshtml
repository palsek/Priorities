@model AspNetIdentityTry1.Models.LogInModel

@{
    ViewBag.Title = "LogIn";
}

@if (TempData["Notification"] != null)
{
    <h4 style="color: red">@TempData["Notification"]</h4>
}

<h2>Log In</h2>

@Html.ValidationSummary(true)

@using (Html.BeginForm())
{ 
    @Html.EditorForModel()
    
    <p>
        <button type="submit" style="margin-top: 5px" class="btn btn-default">Log In</button>
    </p>
}